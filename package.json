    "jm-ez-l10n": "^1.0.0",
    "lint-staged": "^14.0.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.5",
    "ora": "^8.1.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
