{
  "name": "jetdev-assignment",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint-auto-fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "start": "tsc && tsc-alias && node dist",
    "build": "tsc && tsc-alias",
    "env": "sync-dotenv",
    "postinstall": "husky install",
    "liquibase": "cd liquibase && liquibase update",
    "liquibase-prod": "cd liquibase && liquibase update --defaultsFile=./liquibase-prod.properties",
    "liquibase-dev": "cd liquibase && liquibase update --defaultsFile=./liquibase-dev.properties",
    "pretty:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,md}\"",
    "pretty:write": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,md}\"",
    "test-migrate": "cd liquibase && liquibase update --defaultsFile=./liquibase-test.properties ",
    "start-db": "node test-config/start-db.js && cd liquibase && liquibase update --defaultsFile=./liquibase-test.properties",
    "stop-db": "docker-compose down",
    "test": "mocha dist/test/**/*.test.js --timeout 1500000"
  },
  "engines": {
    "node": "^18.20.0"
  },
  "private": true,
  "author": "Dhruv Kapadi",
  "license": "ISC",
